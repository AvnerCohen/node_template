    "status": "naught status run/naught.ipc",
    "deploy": "naught deploy --timeout 15 run/naught.ipc "
  },
  "repository": "",
  "dependencies": {
    "express": "~3.4",
    "graylog2": "~0.1.0",
    "lodash": "~2.2",
    "jade": "~0.35",
    "mongodb": "~1.3",
    "mocha": "~1.13",
    "should": "~2.1",
    "naught": "~1.4.1",
    "supertest": "~0.8.0",
    "statsd-client": "~0.0.15"
  }
}
